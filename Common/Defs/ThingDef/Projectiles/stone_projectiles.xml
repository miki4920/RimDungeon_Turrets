			<texPath>Things/Projectile/Large_Stone</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<thingClass>Projectile_Explosive</thingClass>
		<modExtensions>
			<li Class="RimDungeon.Projectile_Def">
				<shotCount>1</shotCount>
			</li>
		</modExtensions>
